        <Rectangle arcWidth="5.0" fill="#e7e4df" height="45.0" layoutX="151.0" layoutY="177.0" stroke="#95876f" strokeType="INSIDE" width="554.0" />
        <Label alignment="TOP_LEFT" layoutX="172.0" layoutY="181.0" prefHeight="38.0" prefWidth="249.0" text="RAÇA ESCOLHIDA" textFill="#110000">
